{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.8"
            }
        }
    ],
    "routes": [
        {
            "src": "^/(.*)$",
            "has": [
                {
                    "type": "host",
                    "value": "www.spranki.art"
                }
            ],
            "status": 301,
            "headers": {
                "Location": "https://spranki.art/$1"
            }
        },
        {
            "src": "^/(.*)$",
            "has": [
                {
                    "type": "header",
                    "key": "x-forwarded-proto",
                    "value": "http"
                }
            ],
            "status": 301,
            "headers": {
                "Location": "https://spranki.art/$1"
            }
        },
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "git": {
        "deploymentEnabled": {
            "main": true
        }
    }
}
